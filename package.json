{
  "name": "muxmepls",
  "version": "0.6.0",
  "description": "MuxMePls is a nodejs utility to mux \".ass\" subtitles together with the fonts specified in a font folder into mkv container",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "build-win64": "nexe main.js -t win-x64 --build",
    "build-linux64": "nexe main.js -t linux-x64 --build",
    "build-macos64": "nexe main.js -t macos-x64 --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michelangelo1337/MuxMePls.git"
  },
  "keywords": [
    "merge-ass-mkv",
    "mkv-ass-muxer",
    "merge",
    "ass with mkv",
    "font with ass",
    "mux",
    "mux-ass"
  ],
  "author": "Michelangelo1337",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Michelangelo1337/MuxMePls/issues"
  },
  "homepage": "https://github.com/Michelangelo1337/MuxMePls#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "mocha": "^8.3.2",
    "nexe": "^4.0.0-beta.18",
    "prompt-sync": "^4.2.0",
    "yargs": "^16.2.0"
  }
}
